{
    "rewrites": [
      {
        "source": "/:path((?!api/|.*\\.js$).*)",
        "destination": "/index.html"
      },
      {
        "source": "/:path(api/.*)",
        "destination": "/api"
      },
      {
        "source": "/:path((.*\\.js$))",
        "destination": "/dist/:path"
      }
    ]
}
  